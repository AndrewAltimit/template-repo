{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127\\.0\\.0\\.1"
    },
    {
      "pattern": "^http://192\\.168\\."
    },
    {
      "pattern": "^http://0\\.0\\.0\\.0"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "\\{\\{.*\\}\\}"
    },
    {
      "pattern": "^chrome://"
    },
    {
      "pattern": "^file://"
    },
    {
      "pattern": "^ftp://"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/AndrewAltimit/template-repo/blob/main/"
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com",
        "https://raw.githubusercontent.com"
      ],
      "headers": {
        "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8",
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check/3.12.2)"
      }
    },
    {
      "urls": [
        "https://www.linkedin.com"
      ],
      "headers": {
        "Accept": "text/html,application/xhtml+xml",
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200,
    201,
    202,
    203,
    204,
    206,
    301,
    302,
    303,
    307,
    308
  ],
  "projectBaseUrl": "https://github.com/AndrewAltimit/template-repo",
  "ignoreDisable": false
}

{
  "mcpServers": {
    "code-quality": {
      "command": "docker-compose",
      "args": [
        "-f",
        "./docker-compose.yml",
        "--profile",
        "services",
        "run",
        "--rm",
        "-T",
        "mcp-code-quality",
        "python",
        "-m",
        "mcp_code_quality.server",
        "--mode",
        "stdio"
      ]
    },
    "content-creation": {
      "command": "docker-compose",
